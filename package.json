{
	"name": "shortenerurl-api",
	"version": "1.7.0",
	"description": "",
	"main": "dist/index.js",
	"scripts": {
		"test": "jest --no-cache --runInBand",
		"test:dev": "jest --no-cache --runInBand --watchAll",
		"test:cov": "jest --no-cache --runInBand --coverage",
		"test:staged": "jest --passWithNoTests --findRelatedTests",
		"build": "rimraf ./dist && tsc",
		"start": "npm run build && node  -r ts-node/register/transpile-only -r tsconfig-paths/register dist/src/index.js",
		"dev": "ts-node-dev --respawn --transpile-only --inspect --ignore-watch node_modules -r tsconfig-paths/register src/index.ts",
		"release": "standard-version",
		"lint": "eslint . --ext .ts --fix",
		"prettier": "prettier --config .prettierrc src/*.ts --write",
		"lint-staged": "lint-staged",
		"prepare": "husky install"
	},
	"repository": {
		"type": "https://github.com/GustavoCesarSantos/encurtador-api.git"
	},
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/GustavoCesarSantos/encurtador-api/issues"
	},
	"homepage": "https://github.com/GustavoCesarSantos/encurtador-api#readme",
	"devDependencies": {
		"@types/express": "^4.17.13",
		"@types/jest": "^27.4.0",
		"@types/node": "^16.11.12",
		"@typescript-eslint/eslint-plugin": "^5.7.0",
		"@typescript-eslint/parser": "^5.7.0",
		"eslint": "^8.4.1",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-no-loops": "^0.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"husky": "^7.0.4",
		"jest": "^27.5.1",
		"lint-staged": "^12.1.2",
		"prettier": "^2.5.1",
		"prisma": "^3.10.0",
		"rimraf": "^3.0.2",
		"standard-version": "^9.3.2",
		"ts-jest": "^27.1.3",
		"ts-node-dev": "^1.1.8",
		"tsconfig-paths": "^3.14.1",
		"typescript": "^4.5.4"
	},
	"dependencies": {
		"@prisma/client": "^3.10.0",
		"dotenv": "^16.0.0",
		"express": "^4.17.3"
	},
	"standard-version": {
		"releaseCommitMessageFormat": "chore(release): {{currentTag}} [skip ci]"
	}
}
